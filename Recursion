#include<iostream>
#include<string>
using namespace std;
void rep(string s,int i){
  if(s[i]=="\0" || s[i+1]=="\0")
      return;

  if(s[i]=="p" && s[i+1]=="\0"){
	  int j=i+2;
	  while(s[j]!="\0")
	     j++;
	  while(j>=i+2)
	    {s[j+2]=s[j]; j--;}

	  s[i]="3";s[i+1]=".";s[i+2]="1";s[i+3]="4";

  }	  
return rep(s,i+1);
}
int main() {
    int n;cin>>n;cin.get();string s;
	for(int i=0;i<n;i++)
	 {
		 getline(cin,s);
		 string s1[1000]=s;
		 rep(s1,0);
		 cout<<s1<<endl;
	 }
	return 0;
}
